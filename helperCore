                                salesOrderReference: doc.salesOrderReference,
                                url: { text: doc.publicUrl, hyperlink: doc.publicUrl }
                            };

                            if (doc.status === "Valid" && doc.cancelRequestDate != null && doc.declineCancelRequestDate == null)
