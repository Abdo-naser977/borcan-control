                        }

                        worksheet.addRows(rows);

                        worksheet.autoFilter = {
                            from: 'A1',
                            to: {
                                row: rows.length + 1,
                                column: columns.length
                            }
                        }

                        workbook.xlsx.writeBuffer().then(function (data) {
