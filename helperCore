                            { header: 'Ø¥Ø³Ù… Ø§Ù„Ø¨Ø§Ø¦Ø¹', key: 'issuerName', width: 32 },
                            { header: 'Ø¹Ù†ÙˆØ§Ù† Ø§Ù„Ø¨Ø§Ø¦Ø¹', key: 'issuerAddress', width: 32 },
